generator client {
  provider = "prisma-client-js"
}

datasource postgresql {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model rules {
  id    Int      @default(autoincrement()) @id
  admin Boolean?
}

model users {
  id    Int     @default(autoincrement()) @id
  name  String?
  email String?
}
